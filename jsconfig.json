{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "pages/*": [
        "src/pages/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "boot/*": [
        "src/boot/*"
      ],
      "store/*": [
        "src/store/*"
      ],
      "vue$": [
        "src/store/*/vue/dist/vue.runtime.esm-bundler.js"
      ],
    },
    "jsx": "preserve"
  },
  "exclude": [
    "dist",
    ".quasar",
    "node_modules"
  ],
}